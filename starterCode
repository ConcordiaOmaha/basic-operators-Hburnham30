// Example program
#include <iostream>
#include <string>
int main()
{
std::cout << "ALL INPUTS MUST BE INTEGERS\n";
  int num1 = 0; 
  int num2 = 0;
  std::cout << "enter a number:\n"; 
  std::cin >> num1; 
  std::cout << "enter another number:\n";
  std::cin >> num2; 
  int output1 = num1 + num2;
  std::cout << num1 << "+" << num2 << "=" << output1 << "\n";
   int output2 = num1 - num2;
  std::cout << num1 << "-" << num2 << "=" << output2 << "\n";
     int output3 = num1 * num2;
  std::cout << num1 << "*" << num2 << "=" << output3 << "\n";
   int output4 = num1 / num2;
  std::cout << num1 << "/" << num2 << "=" << output4;
  int output5 = num1 % num2;
  std::cout << " Remainder " << output5 << "\n";
}
