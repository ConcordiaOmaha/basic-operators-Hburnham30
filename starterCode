// Example program



#include <iostream>



#include <string>







int main()



{

int num1 = 0; 

  std::cout << "enter a number"; 

  std::cin >> num1; 

  int num2 = 5 + num1; 

  std::cout << " 5 + your number is " << num2 << "\n";

  int num3 = 5 - num1;

  std::cout << " 5 - your number is " << num3 << "\n";

  int num4 = 5 * num1;

  std::cout << " 5 times your number is " << num4 << "\n";

  int num5 = 5 / num1;

  std::cout << " 5 divided by your number is " << num5 << "\n";
  
  int num6 = 5 % num1;
  std::cout << " 5 mod your number is " << num6<< "\n";

  }
